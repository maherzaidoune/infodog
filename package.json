{
  "name": "infodog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "./node_modules/.bin/eslint .",
    "pre-commit": "./node_modules/.bin/eslint .",
    "pre-push": "npm test",
    "doc": "jsdoc -c jsdoc.json",
    "open": "concurrently \"http-server -a localhost -p 1234\" \"open http://localhost:1234/docs\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "./node_modules/.bin/eslint . --fix",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "react": "17.0.1",
    "react-native": "0.64.2",
    "react-native-circular-progress": "^1.3.7",
    "react-native-create-bridge": "^2.0.1",
    "react-native-dialog": "^8.0.1",
    "react-native-svg": "^12.1.1",
    "react-native-svg-charts": "^5.4.0",
    "react-native-vector-icons": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.14.6",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "@types/react-native": "^0.64.10",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^26.6.3",
    "better-docs": "^2.3.2",
    "concurrently": "^6.2.0",
    "eslint": "7.14.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "http-server": "^0.12.3",
    "husky": "^7.0.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.7",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "17.0.1",
    "typescript": "^4.3.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
